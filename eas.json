{
  "cli": {
    "version": ">= 5.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development-sim": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "production": {
      "distribution": "store",
      "developmentClient": false,
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {
      "ios": {
        "bundleIdentifier": "com.tandhjulet.lectimate"
      }
    }
  }
}
